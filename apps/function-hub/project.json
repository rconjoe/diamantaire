{
  "name": "function-hub",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/function-hub",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/function-hub"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/function-hub"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "function-hub:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "function-hub:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "function-hub:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "function-hub:build:production"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/function-hub/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
