{
  "name": "gateway-core",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/gateway/core/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/gateway/core",
        "tsConfig": "libs/gateway/core/tsconfig.lib.json",
        "packageJson": "libs/gateway/core/package.json",
        "main": "libs/gateway/core/src/index.ts",
        "assets": ["libs/gateway/core/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/gateway/core/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/gateway/core/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
