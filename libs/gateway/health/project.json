{
  "name": "gateway-health",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/gateway/health/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/gateway/health",
        "tsConfig": "libs/gateway/health/tsconfig.lib.json",
        "packageJson": "libs/gateway/health/package.json",
        "main": "libs/gateway/health/src/index.ts",
        "assets": ["libs/gateway/health/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/gateway/health/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/gateway/health/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
