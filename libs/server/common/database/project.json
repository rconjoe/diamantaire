{
  "name": "server-common-database",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/server/common/database/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/server/common/database",
        "tsConfig": "libs/server/common/database/tsconfig.lib.json",
        "packageJson": "libs/server/common/database/package.json",
        "main": "libs/server/common/database/src/index.ts",
        "assets": ["libs/server/common/database/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/server/common/database/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/server/common/database/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["backend"]
}
