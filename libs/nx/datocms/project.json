{
  "name": "nx-datocms",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/nx/datocms/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/nx/datocms",
        "main": "libs/nx/datocms/src/index.ts",
        "tsConfig": "libs/nx/datocms/tsconfig.lib.json",
        "assets": [
          "libs/nx/datocms/*.md",
          {
            "input": "./libs/nx/datocms/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./libs/nx/datocms/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./libs/nx/datocms",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./libs/nx/datocms",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "migrations:new": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn run datocms migrations:new"
      }
    },
    "genql": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn run genql --endpoint 'https://graphql.datocms.com' --output './libs/nx/datocms/generated' -H 'Content-Type: application/json' -H 'Authorization: Bearer bb674a2eeebd339f3b979bc50e150f' -S 'BooleanType:boolean' -S 'CustomData:Record<string,unknown>' -S 'Date:string' -S 'DateTime:string' -S 'FloatType:number' -S 'ItemId:string' -S 'IntType:number' -S 'JsonField:unknown', -S 'MetaTagAttributes:Record<string,string>' -S 'UploadId:string'"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/nx/datocms/**/*.ts", "libs/nx/datocms/package.json"]
      }
    }
  },
  "tags": ["nx-plugin", "graphql", "datocms"]
}
